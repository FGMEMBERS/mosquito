 <animation>
  <type>pick</type>
  <object-name>coolflapL</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-adjust</command>
      <property>controls/engines/engine/cowl-flaps-norm</property>
    <step>1.0</step>
    <min>0</min>
    <max>2</max>
    <wrap>1</wrap>
    </binding>
  </action>
</animation>

<animation>
  <type>rotate</type>
  <object-name>coolflapL</object-name>
  <property>controls/engines/engine/cowl-flaps-norm</property>
   <interpolation>
      <entry>
        <ind>0.0</ind>
        <dep>80</dep>
      </entry>
      
      <entry>
        <ind>1.0</ind>
        <dep>0.0</dep>
      </entry>
    </interpolation>
  <center>
   <x-m>0.03868</x-m>
   <y-m>0.0</y-m>
   <z-m>0.08404</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>-1.0</y>
   <z>0.0</z>
  </axis>
 </animation>  

