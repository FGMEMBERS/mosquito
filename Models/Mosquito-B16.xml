<?xml version="1.0"?>

<!-- de Havilland Mosquito by Detlef Faber -->

<PropertyList>

 <path>mosquito.ac</path>

  <nasal>
    <load>
      var update = aircraft.overlay_update.new();
      update.add("Aircraft/mosquito/Models/AI-Liveries", "sim/model/livery/file");
      update.add("Aircraft/Generic/Logos", "sim/multiplay/generic/string");
    </load>
    <unload>
      update.stop();
    </unload>
  </nasal>

	<animation>
		<type>shader</type>
		<shader>chrome</shader>
		<texture>Aircraft/mosquito/Models/glass_shader.png</texture>
		<object-name>canopyglass</object-name>
		<object-name>topdoorglass</object-name>
		<object-name>noseglass</object-name>
	</animation>
	<model>
		<name>cockpit</name>
		<path>Aircraft/mosquito/Models/cockpit.xml</path>
	</model>
	<animation>
  <object-name>cockpit</object-name>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>10</max-m>
	</animation>

<animation>
  <type>rotate</type>
  <object-name>yoke</object-name>
  <object-name>yokewheel</object-name>
  <property>controls/flight/elevator</property>
  <factor>-15</factor>
  <center>
   <x-m>1.935</x-m>
