<?xml version="1.0"?>
  <PropertyList> 
    
    <path>boost20.ac</path>
    

    <animation> 
      <type>rotate</type>
      <object-name>boost-Needle1</object-name>
      <property alias ="../../mp-inhg"/>
      <!-- 4 psi = 8.144 inHg therefore: -->
      <!-- I measured the angles on the texture using gimp. The scale is nonlinear. -->
      <interpolation> 
        <entry><ind>21.776</ind><dep>-44</dep></entry> <!-- -4psi -->
        <entry><ind>29.920</ind><dep>  0</dep></entry> <!-- +0psi -->
        <entry><ind>38.064</ind><dep> 41</dep></entry> <!-- +4psi -->
        <entry><ind>46.208</ind><dep> 84</dep></entry> <!-- +8psi -->
        <entry><ind>54.352</ind><dep>125</dep></entry> <!-- +12psi -->
        <entry><ind>62.496</ind><dep>167</dep></entry> <!-- +16psi -->
        <entry><ind>70.640</ind><dep>212</dep></entry> <!-- +20psi -->
        <entry><ind>78.784</ind><dep>254</dep></entry> <!-- +24psi -->
        <entry><ind>82.856</ind><dep>274</dep></entry> <!-- +26psi -->
      </interpolation>
      <axis>
        <x>-1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
    
    <animation> 
      <type>material</type>
      <emission> 
        <factor-prop>controls/lighting/panel-norm</factor-prop>
        <red>0.75</red>
        <green>0.25</green>
        <blue>0.25</blue>
      </emission>
    </animation>

    <!-- *** LoD *** -->

    <animation>
      <type>range</type>
      <min-m>0</min-m>
      <max-m>5</max-m>
    </animation>
  </PropertyList>
