<?xml version="1.0"?>
  <PropertyList> 
    <path>fuelcontrol.ac</path>
    
 <animation>
  <type>pick</type>
  <object-name>stbcock</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-adjust</command>
      <property>controls/fuel/stbdcock</property>
    <step>1</step>
    <min>0</min>
    <max>3</max>
    <wrap>1</wrap>
    </binding>
    <binding>
      <command>nasal</command>
      <script>mosquito.switch_fuelS()</script>
    </binding>
  </action>
</animation>

 <animation>
  <type>pick</type>
  <object-name>prtcock</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-adjust</command>
      <property>controls/fuel/prtcock</property>
    <step>1</step>
    <min>0</min>
    <max>3</max>
    <wrap>1</wrap>
    </binding>
    <binding>
      <command>nasal</command>
      <script>mosquito.switch_fuelP()</script>
    </binding>
  </action>
</animation>

<animation>
  <type>rotate</type>
  <object-name>stbcock</object-name>
  <property>controls/fuel/stbdcock</property>
  <interpolation>
    <entry>
      <ind>0.0</ind>
      <dep>0</dep>
    </entry>
    <entry>
      <ind>1.00</ind>
      <dep>-90.0</dep>
    </entry>
    <entry>
      <ind>2.0</ind>
      <dep>90</dep>
    </entry>
  </interpolation>
  <center>
   <x-m>0.06276</x-m>
   <y-m>-0.03288</y-m>
   <z-m>-0.03035</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>  

<animation>
  <type>rotate</type>
  <object-name>prtcock</object-name>
  <property>controls/fuel/prtcock</property>
  <interpolation>
    <entry>
      <ind>0.0</ind>
      <dep>0</dep>
    </entry>
    <entry>
      <ind>1.00</ind>
      <dep>90.0</dep>
    </entry>
    <entry>
      <ind>2.0</ind>
      <dep>-90</dep>
    </entry>
  </interpolation>
  <center>
   <x-m>0.06276</x-m>
   <y-m>0.03288</y-m>
   <z-m>-0.03035</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>  

