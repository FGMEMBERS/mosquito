<?xml version="1.0"?>
<PropertyList>

 <path>throttle.ac</path>

<!-- Throttles -->

 <animation>
  <type>rotate</type>
  <object-name>throttleL</object-name>
  <property>controls/engines/engine[0]/throttle</property>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>30</dep>
   </entry>
   <entry>
    <ind>1</ind>
    <dep>-30</dep>
   </entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
  <animation>
     <type>slider</type>
     <object-name>throttleL</object-name>
     <action>
       <binding>
         <command>property-adjust</command>
         <property>controls/engines/engine[0]/throttle</property>
         <factor>0.1</factor>
         <min>0</min>
         <max>1.0</max>                
         <wrap>false</wrap>
       </binding>
     </action>
        <shift-action>
       <binding>
         <command>property-adjust</command>
         <property>controls/engines/engine[0]/throttle</property>
         <factor>0.1</factor>
         <min>0</min>
         <max>1.0</max>                
         <wrap>false</wrap>
       </binding>
       <binding>
         <command>property-adjust</command>
         <property>controls/engines/engine[1]/throttle</property>
         <factor>0.1</factor>
         <min>0</min>
         <max>1.0</max>                
         <wrap>false</wrap>
       </binding>
        </shift-action>
      <hovered>
        <binding>
          <command>set-tooltip</command>
          <tooltip-id>Left throttle</tooltip-id>
          <label>Left throttle = %s</label>
          <property>controls/engines/engine[0]/throttle</property>
        </binding>
      </hovered>
   </animation>

